<section id="contact-me">
  <div class="row remove-margin">
    <div id="contact-info">
      <div id="contact-form" class="col s6">
        <h4>Get in touch</h4>
        <div>
          <i class="fa fa-phone" aria-hidden="true"></i> 919.559.2872
        </div>
        <div>
          <i class="fa fa-envelope" aria-hidden="true"></i> jbburnette2@gmail.com
        </div>
        <form class="col s12">
          <div class="input-field col s12">
            <input placeholder="Placeholder" id="name" type="text" class="validate">
            <label for="name">First Name</label>
          </div>
          <div class="input-field col s12">
            <input placeholder="Placeholder" id="email" type="text" class="validate">
            <label for="email">Email</label>
          </div>
          <div class="input-field col s12">
            <textarea id="message" class="materialize-textarea"></textarea>
            <label for="message">Message</label>
          </div>
          <div class="row remove-margin col s12">
            <button class="right btn waves-effect waves-light" type="submit" name="action">
              Submit
              <i class="fa fa-paper-plane" aria-hidden="true"></i>
            </button>
          </div>
        </form>
      </div>
    </div>
  </div>
</section>
